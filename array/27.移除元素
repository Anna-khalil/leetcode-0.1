//01 暴力算法
class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int size = nums.size();
        for(int i = 0; i < size; i++)
        {
            if(nums[i] == val)
            {
                for(int j=i+1; j < size; j++)
                {
                    nums[j-1] = nums[j];
                }
                size--;
                i--; //注意重新判断该位置上的新元素
            }
        }
        return size;
    }
};

//02 快慢指针
class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int size = nums.size();
        int slow = 0;
        for(int fast = 0; fast < size; fast++)
        {
            if(nums[fast] != val)
            {
                nums[slow] = nums[fast]; //当非目标时，允许slow更新队列
                slow++;
            }
        }

        return slow;
    }
};