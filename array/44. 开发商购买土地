#include<iostream>
#include<vector>
using namespace std;

const int INF = 0x3f3f3f3f;
int main() {
    int n,m;
    cin >> n >> m;
    int sum = 0;
    vector<vector<int>> vec(n,vector<int>(m,0));
    for(int i = 0; i < n ; i++) {
        for(int j = 0; j < m; j++) {
            cin >> vec[i][j];
            sum += vec[i][j];
        }
    }

    int dif = INF;
    
    int cnt = 0;
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < m; j++) {
            cnt += vec[i][j];
            if( j == m - 1) {
                dif = min(dif,abs(sum - cnt - cnt ));
            }
        }
    }

    cnt = 0;
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < n; j++) {
            cnt += vec[j][i];//注意这里的下标 i列j行
            if( j == n - 1) {
                dif = min(dif, abs(sum - cnt - cnt ));
            }
        }
    }

    cout<<dif<<endl;
    return 0;
}