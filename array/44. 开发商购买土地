#include<iostream>
#include<vector>
#include<climits>

using namespace std;

//找错横切/竖切 两者最小的差值
int main()
{
    int n,m;
    int sum=0;
    cin>>n>>m;
    vector<vector<int>> vec(n,vector<int>(m,0));

    for(int i = 0; i < n; i++)
    {
        for(int j = 0; j < m; j++)
        {
            cin >> vec[i][j];
            sum += vec[i][j];
        }
    }

    int cnt = 0;
    int res = INT_MAX;
    for(int i = 0; i < n; i++)
    {
        for(int j = 0; j < m; j++)
        {
            cnt += vec[i][j];
            if(j == m - 1) res = min(res, abs(sum - cnt - cnt));//当扫描到该行最后一个元素时才判断
        }
    }

    cnt = 0;
    for(int j = 0; j < m; j++)
    {
        for(int i = 0; i < n; i++)
        {
            cnt += vec[i][j];
            if(i == n - 1) res = min(res, abs(sum - cnt - cnt));
        }
    }

    cout<<res<<endl;
}